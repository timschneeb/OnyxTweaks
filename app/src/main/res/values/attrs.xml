<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IconPreference">
        <attr name="iconTint" />
    </declare-styleable>

    <declare-styleable name="NumberInputBox">
        <attr name="floatPrecision" format="integer" />
        <attr name="step" format="float" />
        <attr name="value" format="float" />
        <attr name="android:min" format="float" />
        <attr name="android:max" format="float" />

        <attr name="suffixText" format="string" />
        <attr name="hintText" format="string" />
        <attr name="helperText" format="string" />
        <attr name="helperTextEnabled" format="boolean" />
    </declare-styleable>
</resources>