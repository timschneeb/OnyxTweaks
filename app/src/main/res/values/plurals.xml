<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- %1$d are placeholders. Do not translate them. -->
    <plurals name="unit_pixels">
        <item quantity="other">%1$dpx</item>
    </plurals>

    <plurals name="unit_rows">
        <item quantity="one">%1$d row</item>
        <item quantity="other">%1$d rows</item>
    </plurals>

    <plurals name="unit_columns">
        <item quantity="one">%1$d column</item>
        <item quantity="other">%1$d columns</item>
    </plurals>

    <plurals name="unit_icons">
        <item quantity="one">%1$d icon</item>
        <item quantity="other">%1$d icons</item>
    </plurals>

    <plurals name="unit_seconds">
        <item quantity="one">%1$d second</item>
        <item quantity="other">%1$d seconds</item>
    </plurals>

    <plurals name="items_selected">
        <item quantity="one">%1$d item selected</item>
        <item quantity="other">%1$d items selected</item>
    </plurals>

    <plurals name="apps_restart_required">
        <!-- Variable can contain for example: other="SystemUI, Settings, Launcher" or one="SystemUI" -->
        <item quantity="one">%1$s restart required to apply changes. Tap on this message to reload now.</item>
        <item quantity="other">%1$s restart required to apply changes. Tap on this message to reload now.</item>
    </plurals>
</resources>